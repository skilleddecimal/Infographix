{
  "archetype_id": "timeline",
  "display_name": "Timeline",
  "description": "Chronological events along a horizontal or vertical axis",
  "layout_strategy": "flow",
  "primary_direction": "horizontal",
  "element_template": {
    "element_type": "rounded_rect",
    "position_rule": "flow_linear",
    "size_rule": "uniform",
    "size_params": {
      "fixed_width": 1.8,
      "fixed_height": 0.9
    },
    "color_rule": "palette_sequence",
    "corner_radius": 0.1,
    "stroke_width": 1.0,
    "text_alignment": "center",
    "bold_label": true,
    "font_family": "Calibri"
  },
  "connector_template": {
    "pattern": "sequential",
    "style": "arrow",
    "routing": "direct",
    "stroke_width": 1.5
  },
  "constraints": [
    {
      "constraint_type": "min_spacing",
      "params": {
        "horizontal": 0.3,
        "vertical": 0.2
      },
      "priority": 1
    }
  ],
  "supports_overlays": true,
  "supports_nested": false,
  "flow_params": {
    "wrap_after": 8,
    "connector_gap": 0.1
  },
  "min_elements": 2,
  "max_elements": 12,
  "category": "timeline",
  "keywords": ["timeline", "history", "chronological", "events", "milestones", "dates"],
  "example_prompts": [
    "Company history timeline",
    "Project milestones",
    "Product evolution history"
  ]
}
